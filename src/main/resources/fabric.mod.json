{
  "schemaVersion": 1,
  "id": "jarm",
  "version": "${version}",
  "name": "JARM 2.0",
  "description": "JARM but for fabric(will replace the forge one)",
  "authors": ["TNT MAN", "KrazyMiner001"],
  "contact": {
    "homepage": "https://jarm.readthedocs.io",
    "sources": "https://github.com/Suyashtnt/JARM-2.0"
  },
  "license": "MIT",
  "icon": "assets/jarm/icon.png",
  "environment": "*",
  "entrypoints": {
    "main": [
      {
        "adapter": "kotlin",
        "value": "com.tnt_man_inc.jarm.Main"
      }
    ],
    "client": [
      {
        "adapter": "kotlin",
        "value": "com.tnt_man_inc.jarm.ClientInit"
      }
    ]
  },
  "mixins": [],
  "depends": {
    "fabricloader": ">=0.7.4",
    "fabric": "*",
    "minecraft": "1.16.x",
    "fabric-language-kotlin": ">=1.3.50"
  },
  "suggests": {
    "flamingo": "*"
  },
  "custom": {
    "modupdater": {
      "strategy": "github",
      "owner": "SuyashTNT",
      "repository": "JARM-2.0"
    }
  }
}
